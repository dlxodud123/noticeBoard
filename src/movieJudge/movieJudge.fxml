<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="movieJudge.judgeController">
   <children>
      <Label fx:id="judgeName" layoutX="260.0" layoutY="44.0" text="영화">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <ImageView fx:id="judgeProfile" fitHeight="281.0" fitWidth="226.0" layoutX="14.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="252.0" layoutY="550.0" mnemonicParsing="false" onAction="#review" text="평점/리뷰 저장" />
      <TextArea fx:id="judgeText" layoutX="10.0" layoutY="353.0" prefHeight="185.0" prefWidth="580.0" promptText="리뷰를 작성해주세요." />
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backButton" prefHeight="22.0" prefWidth="28.0" text="←" />
   </children>
</AnchorPane>
